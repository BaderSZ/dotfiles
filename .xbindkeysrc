#keystate_capslock = enable



#Mute
"pactl set-sink-mute 0 toggle"
    m:0x0 + c:121
    XF86AudioMute 

#Vol Down
"pactl set-sink-volume alsa_output.pci-0000_00_1b.0.analog-stereo -5%"
    m:0x0 + c:122
    XF86AudioLowerVolume 

#Vol Up
"pactl set-sink-volume alsa_output.pci-0000_00_1b.0.analog-stereo +5%"
    m:0x0 + c:123
    XF86AudioRaiseVolume 

#screenshot
"scrot"
    m:0x0 + c:107
    Print 

#Brightness Down
"xbacklight -5"
    m:0x0 + c:232
    XF86MonBrightnessDown 

#Brightness Up
"xbacklight +5"
    m:0x0 + c:233
    XF86MonBrightnessUp 

#Remark
"Command"
    m:0x40 + c:133
    Mod4 + Super_L 

#
# End of xbindkeys configuration
